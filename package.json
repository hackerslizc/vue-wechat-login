{
  "name": "vue-wechat-login",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "serve": "vite",
    "build": "vite build",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --fix src/**/*.{js,ts,tsx,vue}",
    "lint:style": "stylelint --fix src/**/*.{vue,htm,html,css,sss,less,scss}",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ],
    "src/**/*.{vue,htm,html,css,sss,less,scss}": [
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "vant": "^3.0.9",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "postcss-px-to-viewport": "^1.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.5",
    "vite-plugin-compression": "^0.2.3",
    "vite-plugin-pwa": "^0.5.6",
    "vite-plugin-style-import": "^0.8.1"
  }
}
